{
  "name": "hiv-express",
  "version": "1.0.0",
  "description": "NodeJS backend for hiv",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "tsnd --respawn index.ts",
    "migration:generate": "npx drizzle-kit generate:pg --schema=./src/db/schema/*",
    "migration:run": "node -r esbuild-register src/db/migrate.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/pg": "^8.10.3",
    "drizzle-kit": "^0.19.13",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
